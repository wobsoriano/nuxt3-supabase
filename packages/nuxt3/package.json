{
  "name": "nuxt3-supabase",
  "version": "0.3.0",
  "description": "Nuxt 3 module and composables for Supabase.",
  "keywords": [
    "nuxt",
    "vue",
    "supabase"
  ],
  "license": "MIT",
  "author": {
    "name": "Robert Soriano",
    "email": "sorianorobertc@gmail.com"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/composables.cjs",
      "import": "./dist/composables.mjs"
    },
    "./module": "./dist/module.mjs"
  },
  "main": "./dist/composables.cjs",
  "module": "./dist/composables.mjs",
  "types": "./dist/composables.d.ts",
  "files": [
    "dist/composables.cjs",
    "dist/composables.mjs",
    "dist/composables.d.ts",
    "dist/plugin.mjs",
    "dist/plugin.cjs",
    "dist/plugin.d.ts",
    "dist/module.mjs",
    "dist/module.cjs",
    "dist/module.d.ts"
  ],
  "scripts": {
    "build": "unbuild"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.24.0",
    "nuxt3": "latest",
    "pathe": "^0.2.0"
  },
  "devDependencies": {
    "unbuild": "^0.5.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
